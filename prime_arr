#include <iostream>
#include <cmath>
using namespace std;
int arr[1001] = { 0 };

int main()
{
	int n;
	int max;
	int cnt = 0;
	cin >> n;

	for (int i = 0; i < n; i++)
	{
		cin >> max;
	}
	for (int i = 1; i <= 100;i++)
	{
		for (int j = i*i; j < 1001; j++)
		{
			if (arr[j] == 0 && j % i == 0 && j > i)
				arr[j] = 1;
		}
	}

	for (int i = 2; i < max; i++)
	{
		if (arr[i] == 0)
			cnt++;
	}
	cout << cnt;
	return 0;
}
//
